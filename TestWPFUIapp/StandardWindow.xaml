<Window x:Class="TestWPFUIapp.StandardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestWPFUIapp"
        xmlns:wpfui="clr-namespace:WPFUI.Controls;assembly=WPFUI"
        mc:Ignorable="d"
        Background="#202020"
        
        Title="StandardWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Width="300">
            <TextBlock Foreground="White" Text="Sample text" FontSize="20"/>

            <wpfui:Button Content="WPF UI BUTTON"/>
            <Button Content="BUTTON"/>
            <TextBox Text="Text box"/>
        </StackPanel>
    </Grid>
</Window>
